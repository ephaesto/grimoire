{
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**", "build/**"]
        },
        "test": {
            "dependsOn": ["^test"],
            "outputs": ["coverage/**"]
        },
        "lint": {
            "outputs": []
        },
        "publish": {
            "dependsOn": ["^build", "^test"],
            "outputs": []
        }
    }
}